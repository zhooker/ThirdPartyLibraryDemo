    </com.example.thirdparty.test.TestFrameLayout>

    <com.example.thirdparty.test.TestViewPager
        android:id="@+id/adv_pager"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </com.example.thirdparty.test.TestViewPager>

</LinearLayout>
